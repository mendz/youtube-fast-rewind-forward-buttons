{
    "extends": "@parcel/config-webextension",
    "transformers": {
        "*.{ts,tsx}": [
            "@parcel/transformer-typescript-tsc"
        ],
        "*.svg": [
            "@parcel/transformer-svg"
        ],
        "url:*.svg": [
            "@parcel/transformer-raw"
        ]
    },
    "validators": {
        "*.{ts,tsx}": [
            "@parcel/validator-typescript"
        ]
    }
}