{
  "manifest_version": 2,
  "short_name": "YouTube Rewind&Forward Buttons",
  "name": "YouTube Fast Rewind & Forward Buttons",
  "description": "Add Fast Rewind & Forward buttons to videos player in YouTube site",
  "version": "1.0.5",
  "browser_action": {},
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "icons": {
    "16": "icons/fast-forward-16.png",
    "32": "icons/fast-forward-32.png",
    "48": "icons/fast-forward-48.png",
    "60": "icons/fast-forward-60.png",
    "128": "icons/fast-forward-128.png"
  },
  "permissions": ["webNavigation"]
}
